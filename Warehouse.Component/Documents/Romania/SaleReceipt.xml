<Forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:///e:/Projects/Work/Warehouse/Warehouse.Component/Documenting/FormObjectTemplate.xsd">
  <Form formName="SaleReceipt">
    <Section style="vpadding:10">
      <VBox style="width:100%;innervspacing:10">
        <TextBox sourceField="nrcdReceiptTitle" font="size:14;underline:true;bold:true" style="width:100%;padding:7;halign:center"/>
        <HBox style="width:100%">
            <Table style="width:100%;border:0;innerborder:0;innerhspacing:10" headerStyle="width:100%;vfill:true;bgcolor:#D4D0C8" 
                   itemStyle="width:100%;vfill:true;allowvbreak:false" footerStyle="width:100%;vfill:true;bgcolor:#D4D0C8">
                <Columns>
                  <Column />
                  <Column style="width:100%" />
                </Columns>
                <Rows>
                  <Row style="innervborder:0;spacing:0;innerhspacing:10">
                    <Cell>
                      <TextBox sourceField="dateLabel" font="size:10" />
                    </Cell>
                    <Cell>
                      <TextBox sourceField="receiptDate" font="size:10;bold:true" style="width:100%;hfill:false" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:10">
                    <Cell>
                      <TextBox sourceField="numberLabel" font="size:10" />
                    </Cell>
                    <Cell>
                      <TextBox sourceField="receiptNumber" font="size:10;bold:true" style="width:100%;hfill:false" />
                    </Cell>
                  </Row>
                </Rows>
              </Table>
            <HBox style="width:100%" />
            <Table style="width:100%;border:0;innerborder:0;innerhspacing:10" headerStyle="width:100%;vfill:true;bgcolor:#D4D0C8" 
                   itemStyle="width:100%;vfill:true;allowvbreak:false" footerStyle="width:100%;vfill:true;bgcolor:#D4D0C8">
                <Columns>
                  <Column />
                  <Column style="width:100%" />
                </Columns>
                <Rows>
                  <Row style="innervborder:0;spacing:0;innerhspacing:10">
                    <Cell>
                      <TextBox sourceField="invoiceDateLabel" font="size:10" />
                    </Cell>
                    <Cell>
                      <TextBox sourceField="invoiceDate" font="size:10;bold:true" style="width:100%;hfill:false" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:10">
                    <Cell>
                      <TextBox sourceField="invoiceNumberLabel" font="size:10" />
                    </Cell>
                    <Cell>
                      <TextBox sourceField="invoiceNumber" font="size:10;bold:true" style="width:100%;hfill:false" />
                    </Cell>
                  </Row>
                </Rows>
              </Table>
          </HBox>
        <HBox style="width:100%">
            <Table style="width:50%;innerborder:0;innerhspacing:20" headerStyle="width:100%;vfill:true;bgcolor:#D4D0C8" 
                   itemStyle="width:100%;vfill:true;allowvbreak:false" footerStyle="width:100%;vfill:true;bgcolor:#D4D0C8">
                <Columns>
                  <Column />
                  <Column style="width:100%" />
                </Columns>
                <Rows>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="recipientNameLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="recipientName" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="recipientNumberLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="recipientNumber" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="recipientCityLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="recipientCity" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="recipientAddressLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="recipientAddress" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="recipientTelephoneLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="recipientTelephone" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="recipientLiablePersonLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="recipientLiablePerson" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                </Rows>
              </Table>
            <HBox style="width:10" />
            <Table style="width:50%;innerborder:0;innerhspacing:20" headerStyle="width:100%;vfill:true;bgcolor:#D4D0C8" 
                   itemStyle="width:100%;vfill:true;allowvbreak:false" footerStyle="width:100%;vfill:true;bgcolor:#D4D0C8">
                <Columns>
                  <Column />
                  <Column style="width:100%" />
                </Columns>
                <Rows>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="supplierNameLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="supplierName" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="supplierNumberLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="supplierNumber" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="supplierCityLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="supplierCity" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="supplierAddressLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="supplierAddress" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="supplierTelephoneLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="supplierTelephone" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="supplierLiablePersonLabel" />
                    </Cell>
                    <Cell style="width:100%">
                      <TextBox sourceField="supplierLiablePerson" wrap="True" font="bold:true" style="width:100%" />
                    </Cell>
                  </Row>
                </Rows>
              </Table>
          </HBox>
        <TextBox sourceField="pointOfSale" sourceFormat="pointOfSaleFormat" font="bold:true" style="width:100%;padding:2"/>
      </VBox>
    </Section>
    <Section style="vpadding:8">
      <Table sourceTable="receiptDetails" style="width:100%" headerStyle="width:100%;vfill:true;bgcolor:#D4D0C8" itemStyle="width:100%;vfill:true;allowvbreak:false" footerStyle="width:100%;vfill:true;bgcolor:#D4D0C8">
        <Columns>
          <Column>
            <Template style="padding:2;halign:center;valign:center;height:100%">
              <TextBox sourceField="Number" />
            </Template>
            <Header style="padding:2">
              <TextBox sourceField="detailHeaderNumber" />
            </Header>
          </Column>
          <Column>
            <Template style="padding:2;valign:center">
              <TextBox sourceField="GoodsCode" />
            </Template>
            <Header style="padding:2">
              <TextBox sourceField="detailHeaderCode" />
            </Header>
          </Column>
          <Column>
            <Template style="width:100%;padding:2;valign:center">
              <TextBox sourceField="GoodsName" wrap="true" />
            </Template>
            <Header style="width:100%;padding:2;hfill:false">
              <TextBox sourceField="detailHeaderGoods" />
            </Header>
          </Column>
          <Column>
            <Template style="padding:2;halign:center;valign:center">
              <TextBox sourceField="MUnit" />
            </Template>
            <Header style="padding:2">
              <TextBox sourceField="detailHeaderMUnit" />
            </Header>
          </Column>
          <Column>
            <Template style="padding:2;halign:right;valign:center">
              <TextBox sourceField="Quantity" />
            </Template>
            <Header style="padding:2">
              <TextBox sourceField="detailHeaderQtty" />
            </Header>
          </Column>
          <Column>
            <Template style="padding:2;halign:right;valign:center">
              <TextBox sourceField="Price" />
            </Template>
            <Header style="padding:2">
              <TextBox sourceField="detailHeaderPrice" />
            </Header>
          </Column>
          <Column>
            <Template style="padding:2;halign:right;valign:center">
              <TextBox sourceField="Total" />
            </Template>
            <Header style="padding:2">
              <TextBox sourceField="detailHeaderTotal" />
            </Header>
          </Column>
        </Columns>
      </Table>
    </Section>
    <Section>
      <VBox style="width:100%">
        <HBox style="width:100%">
            <HBox style="width:100%" />
            <Table style="border:0;innerborder:0;innerhspacing:20;hfill:true" headerStyle="width:100%;vfill:true;bgcolor:#D4D0C8" 
                   itemStyle="width:100%;vfill:true;allowvbreak:false" footerStyle="width:100%;vfill:true;bgcolor:#D4D0C8">
                <Columns>
                  <Column />
                  <Column />
                </Columns>
                <Rows>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="totalLabel" font="size:10;italic:true" />
                    </Cell>
                    <Cell style="halign:right">
                      <TextBox sourceField="total" font="size:10;bold:true" style="halign:right" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="vatLabel" font="size:10;italic:true" />
                    </Cell>
                    <Cell style="halign:right">
                      <TextBox sourceField="vat" font="size:10;bold:true" style="halign:right" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell />
                    <Cell style="height:7;valign:center">
                      <HLine thickness="2" style="width:100%" />
                    </Cell>
                  </Row>
                  <Row style="innervborder:0;spacing:0;innerhspacing:20">
                    <Cell>
                      <TextBox sourceField="totalPlusVatLabel" font="size:10;italic:true" />
                    </Cell>
                    <Cell style="halign:right">
                      <TextBox sourceField="totalPlusVat" font="size:10;bold:true" style="halign:right" />
                    </Cell>
                  </Row>
                </Rows>
              </Table>
          </HBox>
        <HBox style="width:100%;vpadding:10">
          <TextBox sourceField="totalInWordsLabel" font="size:10;underline:false;bold:false;italic:true" />
          <TextBox sourceField="totalInWords" font="size:10;underline:false;bold:true;italic:false" wrap="true" style="width:100%;height:100%" />
        </HBox>
        <VBox style="width:100%;height:100%;vpadding:20">
          <TextBox sourceField="noteLabel" font="underline:true;bold:true;italic:false" />
          <TextBox sourceField="note" style="vpadding:4" />
        </VBox>
        <HBox style="width:100%">
          <HBox style="width:100%;hfill:true">
            <TextBox sourceField="recipientSignLabel" font="size:10;bold:true" />
            <HLine style="width:100%;height:100%;valign:bottom" />
          </HBox>
          <BoxItem style="width:10;height:100%" />
          <HBox style="width:100%;hfill:true">
            <TextBox sourceField="creatorSignLabel" font="size:10;bold:true" />
            <HLine style="width:100%;height:100%;valign:bottom" />
          </HBox>
          <BoxItem style="width:10;height:100%" />
          <HBox style="width:100%;hfill:true">
            <TextBox sourceField="supplierSignLabel" font="size:10;bold:true" />
            <HLine style="width:100%;height:100%;valign:bottom" />
          </HBox>
        </HBox>
      </VBox>
    </Section>
    <Section type="PageFooter">
      <HBox style="width:100%;height:50">
        <TextBox sourceField="productId" font="italic:true" style="width:100%;height:100%;valign:center"/>
        <HBox style="width:100%;valign:center;height:100%;hpadding:10;hfill:true;halign:right">
            <TextBox sourceField="%CurrentPage%" sourceFormat="currentPageFormat"/>
            <TextBox sourceField="%TotalPages%" sourceFormat="totalPagesFormat" style="hpadding:2"/>
          </HBox>
      </HBox>
    </Section>
  </Form>
</Forms>